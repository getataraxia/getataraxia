{
  "presets": ["next/babel"],
  "plugins": [
    [
      "babel-plugin-styled-components",
      {
        "ssr": true
      }
    ],
    [
      "module-resolver",
      {
        "root": ["."],
        "extensions": [
          ".js",
          ".ts",
          ".tsx"
        ]
      }
    ],
    ["inline-react-svg"]
  ]
}
